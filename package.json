{
    "name": "vue-eyein-translation",
    "version": "5.0.6",
    "description": "Vue 2-3 translation plugin for Vite",
    "author": "Alexandre CHENIEUX",
    "license": "BSD-3-CLAUSE",
    "type": "module",
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist",
        "src",
        "vue2.js",
        "vue3.js",
        "vite-plugin-vue-eyein-translation.js"
    ],
    "scripts": {
        "start": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
        "build": "nuxt-module-build build",
        "lint": "eslint .",
        "prepublish": "npm run build"
    },
    "dependencies": {
        "@nuxt/kit": "^3.14.1592",
        "magic-string": "^0.30.14"
    },
    "devDependencies": {
        "@nuxt/devtools": "^1.6.3",
        "@nuxt/eslint-config": "^0.7.2",
        "@nuxt/module-builder": "^0.8.4",
        "@nuxt/schema": "^3.14.1592",
        "@nuxt/test-utils": "^3.15.1",
        "@types/node": "latest",
        "changelogen": "^0.5.7",
        "eslint": "^9.16.0",
        "nuxt": "^3.14.1592",
        "typescript": "~5.6.3",
        "vitest": "^2.1.8",
        "vue-tsc": "^2.1.10"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Eye-InMedia/vue-eyein-translation.git"
    },
    "bugs": {
        "url": "https://github.com/Eye-InMedia/vue-eyein-translation/issues"
    },
    "keywords": [
        "vue",
        "vite",
        "i18n",
        "translation"
    ],
    "engines": {
        "node": ">= 16"
