    "peerDependencies": {
        "vite": ">= 2.5.4",
        "vue": ">= 2.7.0"
    },
    "exports": {
        ".": {
            "types": "./dist/types.d.ts",
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        },
        "./vue2.js": "./vue2.js",
        "./vue3.js": "./vue3.js",
        "./vite-plugin-vue-eyein-translation.js": "./vite-plugin-vue-eyein-translation.js"
    }
